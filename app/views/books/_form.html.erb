<%= form_for @book do |f| %>
	<%= f.label :author %>
	<%= f.text_field :author %>	

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :description %>
    <%= f.text_area :description %>

    <%= f.label :picture %>
    <%= f.file_field :picture %>

    <%= f.submit %>
  <% end %>